{
  "name": "dms-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.1",
  "scripts": {
    "dev": "bash scripts/dev.sh",
    "dev:frontend": "pnpm --filter @dms/app dev",
    "build": "pnpm --filter @dms/app build",
    "test": "bash scripts/test.sh",
    "test:only": "pnpm -r test",
    "test:e2e": "bash scripts/test-e2e.sh",
    "test:all": "bash scripts/test.sh && bash scripts/test-e2e.sh",
    "lint": "pnpm -r lint",
    "supabase": "supabase"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@supabase/supabase-js": "^2.95.3"
  }
}
